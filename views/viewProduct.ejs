<% include partials/header %>

<div class="container">
    <table class="table table-striped table-bordered table-list">
        <thead>
            <tr>
                <th></th>
                <th>Product Number</th>
                <th>Entry Date</th>
                <th>Still Valid?</th>
                <th>Alternatives</th>
                <th>Quantity Available</th>
            </tr> 
        </thead>
      	<tbody>

      		<% var x=0; prods.forEach(function(prod){ %>
      		<tr>
      			<td><a href="/product/activity/<%=prod.productNum%>">View Activity</a></td>
      			<td><a href="/product/show/<%=prod.productNum%>"><%=prod.productNum%></a></td>
      			<td><%=prod.entryDate%></td>
      			<td><%=prod.stillValid%></td>
      			<td><%=prod.alternatives%></td>
      			<td><%=prod.QTY%></td>
      		</tr>
      		<% }); %>
      	</tbody>

<% include partials/footer %>