<% include partials/header %>

<div class="container">
    <table class="table table-striped table-bordered table-list">
        <thead>
            <tr>
                <th></th>
                <th>Part Number</th>
                <th>Entry Date</th>
                <th>Batch Code</th>
                <th>OEM Number</th>
                <th>Quantity</th>
                <th>SRP</th>
            </tr> 
        </thead>
      	<tbody>

      		<% prods.forEach(function(prod){ %>
      		<tr>
      			<td></td>
      			<td><a href="/product/show/<%=prod.productNum%>"><%=prod.productNum%></a></td>
      			<td><a href="/product/activity/<%=prod.productNum%>"><%=prod.entryDate%></a></td>
      			<td><a href="/batch/show/<%=prod.batchCode%>"><%=prod.batchCode%></a></td>
      			<td><%=prod.oemNum%></td>
      			<td><%=prod.QTY%></td>
      			<td><%=prod.srp%></td>
      		</tr>
      		<% }); %>
      	</tbody>




<% include partials/footer %>