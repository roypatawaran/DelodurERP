<% include partials/header %>

<div class="container">
    <div class="row">
        <h1 style="text-align: center;"> Create New Product Entry (Batch) </h1>
        <div style="width: 30%; margin: 30px auto;">
            <form id="formid" action="/product/search2" method="POST">
            	<div class="form-group">
                   <input id="prtNum" readonly class="form-control" name="partNum" type="text" value="<%=pass%>"> 
                </div>
                <div class="form-group">
                    <select id="S00" name="brand">
                          <option disabled selected value> -- Select Brand -- </option>
                          <% prods.forEach(function(prod){ %>
                          <option  value="<%=prod.brand %>"> <%=prod.brand%> </option>
                          <% }); %>
                    </select>
                </div>
                <div class="form-group">
                   <input id="prodNum" readonly class="form-control" name="prodNum" type="text" value=""> 
                </div>
                <button type="submit" class="btn btn-primary btn-lg">SUBMIT</button>
            </form>
            <a style="text-align: center;" href="/">Go Back</a>
        </div>
    </div>

</div>

<script src="/js/productSearch.js"></script>

<% include partials/footer %>
